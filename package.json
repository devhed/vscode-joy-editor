{
    "name": "vscode-joy-editor",
    "displayName": "Joy Editor",
    "description": "vscode joy editor extension",
    "icon": "logo/128x128.png",
    "version": "0.0.9",
    "publisher": "anthonytamez",
    "author": {
        "name": "Anthony Tamez",
        "email": "anthony.tamez@archarithms.com"
    },
    "license": "MIT",
    "homepage": "https://github.com/Archarithms/vscode-joy-editor",
    "repository": {
        "type": "git",
        "url": "https://github.com/Archarithms/vscode-joy-editor"
    },
    "bugs": {
        "url": "https://github.com/Archarithms/vscode-joy-editor"
    },
    "galleryBanner": {
        "color": "#35364E",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Debuggers"
    ],
    "activationEvents": [
        "onCommand:extension.openJoyEditor"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.openJoyEditor",
                "title": "Joy Editor: Launch the editor"
            },
            {
                "command": "extension.reloadJoyEditor",
                "title": "Joy Editor: Reload the editor from the context of the current .joy file"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Joy Editor Configuration",
            "properties": {
                "reduxdev.hostname": {
                    "type": "string",
                    "default": "127.0.0.1",
                    "description": "The hostname your remotedev server started on."
                },
                "reduxdev.socketPort": {
                    "type": "number",
                    "default": 1024,
                    "description": "The socket port for Redux Devtools to listen."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {}
}